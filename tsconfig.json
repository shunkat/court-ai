{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "esModuleInterop": true,
    "composite": true,
    "declaration": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true
  }
}
